					},
					"connectVia": {
						"referenceName": "AutoResolveIntegrationRuntime",
						"type": "IntegrationRuntimeReference"
					},
					"method": "POST",
					"body": {
						"value": "@concat('{\"EmailTo\":\"',activity('lkp_GetEmailDetails').output.firstRow.EmailTo,'\",\"EmailCC\":\"',activity('lkp_GetEmailDetails').output.firstRow.EmailCC,'\",\"EmailBCC\":\"',activity('lkp_GetEmailDetails').output.firstRow.EmailBCC,'\",\"EmailSubject\":\"',activity('lkp_GetEmailDetails').output.firstRow.EmailSubject,'Failed\",\"EmailPriority\":\"',activity('lkp_GetEmailDetails').output.firstRow.EmailPriority,'\",\"EmailBody\":\"Hi Team, <br/><br/> Data Load Process failed at copy activity. <br/><br/>Regards,<br/>Data Support Team\"}')",
						"type": "Expression"
					}
				}
			},
